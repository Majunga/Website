{
  "ConnectionStrings": {
    "DefaultConnection": "#{connectionstring}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Majunga.Client": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "File",
      "FilePath": "https/Majunga.Server.pfx",
      "Password": "#{certPassword}#",
      "Name": "/CN=Majunga.Server"
    }
  },
  "AllowedHosts": "*"
}
